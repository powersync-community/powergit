{
  "name": "@app/explorer",
  "version": "0.0.4",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node ./scripts/dev-with-stack.mjs",
    "dev:remote": "vite",
    "build": "tsc -p tsconfig.json && vite build",
    "build:prod": "STACK_PROFILE=prod pnpm run build",
    "preview": "vite preview",
    "lint": "echo 'stub'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:local": "pnpm test:e2e",
    "test:e2e:staging": "STACK_PROFILE=staging pnpm test:e2e",
    "test:e2e:staging:ci": "STACK_PROFILE=staging pnpm exec playwright test",
    "dev:local": "VITE_POWERSYNC_USE_DAEMON=true vite",
    "dev:prod": "VITE_POWERSYNC_USE_DAEMON=false vite --mode production --port 5784"
  },
  "dependencies": {
    "@isomorphic-git/lightning-fs": "^4.6.2",
    "@journeyapps/wa-sqlite": "^1.4.1",
    "@monaco-editor/react": "^4.6.0",
    "@powersync/common": "1.44.0",
    "@powersync/react": "1.8.2",
    "@powersync/web": "1.30.0",
    "@supabase/supabase-js": "^2.48.1",
    "@tanstack/db": "^0.4.20",
    "@tanstack/powersync-db-collection": "^0.1.16",
    "@tanstack/react-db": "^0.1.43",
    "@tanstack/react-router": "^1.60.0",
    "diff": "^7.0.0",
    "isomorphic-git": "^1.34.2",
    "monaco-editor": "^0.54.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^9.0.1",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "react-spinners": "^0.14.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.0.9",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "^14.2.0",
    "@types/node": "^20.16.5",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.19",
    "fake-indexeddb": "^6.2.4",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.38",
    "tailwindcss": "4.1.16",
    "typescript": "^5.5.0",
    "vite": "^7.2.1",
    "vite-plugin-top-level-await": "^1.4.0",
    "vite-plugin-wasm": "^3.3.3",
    "vitest": "^4.0.7"
  }
}
