{
  "name": "@powersync-community/powergit-core",
  "version": "1.2.2",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./powersync/schema": {
      "import": "./dist/powersync/schema.js",
      "types": "./dist/powersync/schema.d.ts"
    },
    "./powersync/streams": {
      "import": "./dist/powersync/streams.js",
      "types": "./dist/powersync/streams.d.ts"
    },
    "./node": {
      "import": "./dist/node.js",
      "types": "./dist/node.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepack": "npm run build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "echo 'stub'",
    "test": "vitest run"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "@types/node": "^22.7.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.48.1"
  }
}
