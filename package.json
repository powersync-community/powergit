{
  "name": "powersync-git-monorepo",
  "private": true,
  "version": "0.4.0",
  "packageManager": "pnpm@10.18.3",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm --filter @app/explorer dev",
    "dev:prod": "pnpm --filter @app/explorer dev:prod",
    "dev:daemon": "node ./scripts/start-daemon-with-profile.mjs",
    "dev:stack": "node ./scripts/dev-local-stack.mjs",
    "dev:stack:up": "node ./scripts/dev-stack-up.mjs",
    "dev:stack:down": "node ./scripts/dev-stack-down.mjs",
    "build:all": "node ./scripts/build-all.mjs",
    "seed:streams": "node ./scripts/seed-sync-rules.mjs",
    "schema:migrate": "node ./scripts/schema-to-migration.mjs && pnpx supabase db push",
    "lint": "pnpm -r run lint",
    "typecheck": "pnpm -r run typecheck",
    "test": "pnpm -r run test",
    "test:e2e": "pnpm --filter @app/explorer test:e2e",
    "live:validate": "node ./scripts/run-live-validation.mjs",
    "prod:supabase:link": "pnpx supabase link",
    "prod:supabase:status": "pnpx supabase status",
    "prod:supabase:db:push": "pnpx supabase db push",
    "prod:supabase:db:pull": "pnpx supabase db pull",
    "prod:supabase:db:reset": "pnpx supabase db reset",
    "prod:supabase:start": "pnpx supabase start",
    "prod:supabase:stop": "pnpx supabase stop",
    "prod:migrate": "pnpx supabase db push",
    "prod:migrate:reset": "pnpx supabase db reset",
    "postinstall": "node ./scripts/ensure-powersync-core.mjs"
  },
  "pnpm": {
    "overrides": {
      "@tanstack/db": "workspace:*",
      "@tanstack/react-db": "workspace:*",
      "@tanstack/powersync-db-collection": "workspace:*",
      "@powersync/common": "1.40.0",
      "@powersync/node": "0.11.1",
      "@powersync/web": "1.27.1",
      "@powersync/react": "1.8.1"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "@powersync/common": ">=1.40.0"
      }
    },
    "onlyBuiltDependencies": [
      "@journeyapps/wa-sqlite",
      "@powersync/better-sqlite3",
      "@powersync/node",
      "@swc/core",
      "better-sqlite3",
      "esbuild",
      "supabase"
    ],
    "patchedDependencies": {
      "@powersync/web": "patches/@powersync__web.patch",
      "@powersync/node": "patches/@powersync__node.patch"
    }
  },
  "devDependencies": {
    "@pkg/remote-helper": "workspace:*",
    "supabase": "^2.53.6",
    "tsx": "^4.20.6"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.48.1",
    "@tanstack/db": "workspace:*",
    "better-sqlite3": "^12.4.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "yaml": "^2.6.0"
  }
}
