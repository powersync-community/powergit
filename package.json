{
  "name": "powersync-git-monorepo",
  "private": true,
  "version": "0.4.0",
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "node ./scripts/run-with-local-env.mjs pnpm --filter @app/explorer dev",
    "dev:prod": "node ./scripts/run-with-local-env.mjs pnpm --filter @app/explorer dev:prod",
    "dev:daemon": "node ./scripts/run-with-local-env.mjs node ./scripts/start-daemon-with-profile.mjs",
    "dev:stack": "node ./scripts/dev-local-stack.mjs",
    "dev:stack:up": "node ./scripts/dev-stack-up.mjs",
    "dev:stack:down": "node ./scripts/dev-stack-down.mjs",
    "build:all": "node ./scripts/build-all.mjs",
    "seed:streams": "node ./scripts/seed-sync-rules.mjs",
    "schema:migrate": "node ./scripts/schema-to-migration.mjs && pnpx supabase db push",
    "lint": "pnpm -r run lint",
    "typecheck": "pnpm -r run typecheck",
    "test": "pnpm -r --workspace-concurrency 1 test",
    "test:e2e": "pnpm --filter @app/explorer test:e2e",
    "live:validate": "node ./scripts/run-live-validation.mjs",
    "prod:supabase:link": "pnpx supabase link",
    "prod:supabase:status": "pnpx supabase status",
    "prod:supabase:db:push": "pnpx supabase db push",
    "prod:supabase:db:pull": "pnpx supabase db pull",
    "prod:supabase:db:reset": "pnpx supabase db reset",
    "prod:supabase:start": "pnpx supabase start",
    "prod:supabase:stop": "pnpx supabase stop",
    "prod:supabase:functions:list": "pnpx supabase functions list",
    "prod:supabase:functions:deploy": "pnpx supabase functions deploy",
    "prod:supabase:functions:deploy:github-import": "pnpx supabase functions deploy github-import",
    "prod:migrate": "pnpx supabase db push",
    "prod:migrate:reset": "pnpx supabase db reset",
    "create:test-user": "node ./scripts/create-test-user.mjs"
  },
  "pnpm": {
    "overrides": {
      "@tanstack/db": "^0.4.20",
      "@tanstack/react-db": "^0.1.43",
      "@tanstack/powersync-db-collection": "^0.1.16",
      "@powersync/common": "1.44.0",
      "@powersync/node": "0.15.0",
      "@powersync/web": "1.30.0",
      "@powersync/react": "1.8.2"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "@powersync/common": ">=1.40.0"
      }
    },
    "onlyBuiltDependencies": [
      "@journeyapps/wa-sqlite",
      "@powersync/better-sqlite3",
      "@powersync/node",
      "@swc/core",
      "better-sqlite3",
      "esbuild",
      "supabase"
    ]
  },
  "devDependencies": {
    "@powersync-community/powergit-remote-helper": "workspace:*",
    "supabase": "^2.66.0",
    "tsx": "^4.20.6"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.48.1",
    "@tanstack/db": "^0.4.20",
    "better-sqlite3": "^12.4.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "remark-gfm": "^4.0.1",
    "yaml": "^2.6.0"
  }
}
