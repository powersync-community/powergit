{
  "name": "powersync-git-monorepo",
  "private": true,
  "version": "0.4.0",
  "packageManager": "pnpm@10.18.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm --filter @app/explorer dev",
    "dev:stack": "node ./scripts/dev-local-stack.mjs",
    "lint": "pnpm -r run lint",
    "typecheck": "pnpm -r run typecheck",
    "test": "pnpm -r run test",
    "test:e2e": "pnpm --filter @app/explorer test:e2e"
  },
  "pnpm": {
    "overrides": {
      "@tanstack/powersync-db-collection": "github:powersync-ja/temp-tanstack-db#c887d90"
    }
  },
  "devDependencies": {
    "tsx": "^4.20.6"
  },
  "dependencies": {
    "@tanstack/db": "0.4.3",
    "jsonwebtoken": "^9.0.2"
  }
}
